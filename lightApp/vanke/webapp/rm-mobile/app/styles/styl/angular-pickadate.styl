.pickadate {
  z-index 100
  position: fixed;
}

.pickadate a {
  color: #009949;
  text-decoration: none;
}

.pickadate-mask {
  background-color: rgba(0, 0, 0, 0.5);
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 1000;
}

.pickadate-container {
  z-index: 1001;
  background-color: #fff;
  position: relative;
  -webkit-transition: all .3s linear;
  transition: all .3s linear;
}

.pickadate-header {
  position: relative;
  height: 44px;
  line-height: 44px;
}

.pickadate-body {
  padding-bottom: 10px;
}

.pickadate-main {
  margin: 0;
  padding: 0;
  width: 100%;
  text-align: center;
  font-size: 14px;
}

.pickadate-cell {
  overflow: hidden;
  margin: 0 auto;
  padding: 0;
  width: 90%;
}

.pickadate-cell li {
  display: block;
  float: left;
  width: 14.285%;
  padding: 0;
  line-height: 22px;
  text-align: center;
  color: #333;
}

.pickadate-cell li:nth-child(7n+0) {
  color: #E67327
}

.pickadate-cell li:nth-child(1), .pickadate-cell li:nth-child(8), .pickadate-cell li:nth-child(15), .pickadate-cell li:nth-child(22), .pickadate-cell li:nth-child(29), .pickadate-cell li:nth-child(36) {
  color: #E67327
}

.pickadate-cell.pickadate-dates li.pickadate-hide {
  opacity: 0;
}

.pickadate-cell .pickadate-disabled {
  opacity: 0.2;
}

.pickadate-cell .pickadate-disabled a {
  color: #DCDCDC;
}

.pickadate-cell .pickadate-enabled {
  cursor: pointer;
  color: #666666;
}

.pickadate-cell .pickadate-today:before, .pickadate-cell .pickadate-active:before, .pickadate-cell .pickadate-unavailable:before {
  width: 28px;
  height: 28px;
  position: absolute;
  content: "";
  display: block;
  top: 6px;
  left: 50%;
  margin-left: -14px;
  -webkit-border-radius: 14px;
}

.pickadate-cell .pickadate-unavailable:before {
  background-color: rgba(0, 0, 0, 0.3);
}

.pickadate-cell .pickadate-today:before {
  background-color: rgba(234, 234, 234, 0.4);
}

.pickadate-cell .pickadate-active:before {
  background-color: rgba(239, 181, 22, 0.4);
}

.pickadate-cell.pickadate-week {
  background-color: #ddd;
  margin: 10px auto;
  -webkit-border-radius: 12px;
  padding: 2px 0;
}

.pickadate-cell.pickadate-dates li {
  padding: 8px 0;
  position: relative;
}

.pickadate-centered-heading {
  font-weight: normal;
  text-align: center;
  font-size: 16px;
  margin: 0;
  line-height: normal;
  overflow: hidden;
  line-height: 44px;
}

.pickadate-controls {
  z-index: 10;
  width: 90%;
  margin: 0 auto;
}

.pickadate-controls .pickadate-next {
  float: right;
}

.pickadate-controls .pickadate-prev {
  float: left;
}

.pickadate-controls a.disabled {
  color: #ccc;
}

.pickadate-modal {
  position: fixed;
  left: 0;
  right: 0;
}

