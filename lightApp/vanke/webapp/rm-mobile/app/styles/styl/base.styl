/* overflow ������� */
.oh {
  overflow: hidden;
}

.oa {
  overflow: auto;
}

.te {
  -webkit-text-overflow: ellipsis;
  text-overflow: ellipsis;
}

.op0 {
  opacity: 0;
}

.op_5 {
  opacity: .5;
}

.rm-tabbar {
  display: flex;
  border: 1px solid #009949;
  border-radius: 5px;
  background-color: #fff;
  overflow: hidden;
}

.rm-tabbar > li {
  flex: 1;
}

.rm-tabbar > li > a {
  margin-left: -1px;
  display: block;
  text-align: center;
  border-right: 1px solid #009949;
  font-size: 14px;
  line-height: 1.6;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding: 3px 0;
  text-decoration: none;
  color: #009949;
  transition: all ease .3s;
}

.rm-tabbar > li:last-child > a {
  border-right: none;
}

.rm-tabbar > li.active > a {
  color: #fff;
  background-color: #009949;
}