    <entry key="Profile.oauth.refresh_token_grant_type">refresh_token</entry>

    <!--oauth2.0回调地址-->
    <entry key="Profile.oauth.redirect_uri">http://tvkcrm.vankeservice.com/oauth/token</entry>

    <!--oauth2.0返回值-->
