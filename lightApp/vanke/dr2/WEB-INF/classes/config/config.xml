    <entry key="Profile.oauth.client_secret">db37fa14a63c6ca61debc9d7baa46eed</entry>

    <!--oauth2.0认证地址-->
    <entry key="Profile.oauth.url">http://test.4009515151.com/api/lebang/oauth/authorize</entry>

    <!--oauth2.0权限范围-->
    <entry key="Profile.oauth.scopes">r-staff</entry>

