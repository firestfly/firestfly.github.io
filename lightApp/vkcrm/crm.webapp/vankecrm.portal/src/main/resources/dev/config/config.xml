<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment>通用配置信息</comment>

    <!--SSO单点登录的地址-->
    <entry key="Profile.SSO.Server_Url">http://localhost:8080/sso/openid</entry>

    <!--静态资源站点-->
    <entry key="staticWeb">http://static.tvkcrm.vankeservice.com</entry>

    <!--系统登录模式，local：本地登录-->
    <entry key="Profile.Security.Login_Mode">oauth</entry>

    <!--oauth2.0客户端ID-->
    <entry key="Profile.oauth.client_id">f48d1623753669091c40b0d426935bdc</entry>

    <!--oauth2.0客户端密钥-->
    <entry key="Profile.oauth.client_secret">db37fa14a63c6ca61debc9d7baa46eed</entry>

    <!--oauth2.0认证地址-->
    <entry key="Profile.oauth.url">http://wyapp.vanke.com/api/lebang/oauth/authorize</entry>

    <!--oauth2.0权限范围-->
    <entry key="Profile.oauth.scopes">r-staff</entry>

    <!--oauth2.0获取access_token权限类型-->
    <entry key="Profile.oauth.access_token_grant_type">authorization_code</entry>

    <!--oauth2.0刷新access_token权限类型-->
    <entry key="Profile.oauth.refresh_token_grant_type">refresh_token</entry>

    <!--oauth2.0回调地址-->
    <entry key="Profile.oauth.redirect_uri">http://tvkcrm.vankeservice.com/oauth/token</entry>

    <!--oauth2.0返回值-->
    <entry key="Profile.oauth.response_type">code</entry>

    <!--oauth2.0获取access_token地址-->
    <entry key="Profile.oauth.access_token_url">http://wyapp.vanke.com/api/lebang/oauth/access_token</entry>

    <!--oauth2.0刷新access_token地址-->
    <entry key="Profile.oauth.user_info_url">http://wyapp.vanke.com/api/lebang/staffs/me?access_token=</entry>


</properties>